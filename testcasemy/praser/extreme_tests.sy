// 测试样例：deeply_nested_if_else.sy

int main() {
    int x = 0;

    if (x == 0)
        if (x > -1)
            if (x < 1)
                if (x != 2)
                    if (x == x)
                        if (x >= 0)
                            if (x <= 0)
                                x = 1;
                            else
                                x = 2;
                        else
                            x = 3;
                    else
                        x = 4;
                else
                    x = 5;
            else
                x = 6;
        else
            x = 7;
    else
        x = 8;

    putint(x); // 输出结果应为 1
    return 0;
}

