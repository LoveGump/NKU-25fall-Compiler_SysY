// 测试样例：deeply_nested_loops.sy

int main() {
    int i, j, k;

    for (i = 0; i < 2; i=i+1) {
        for (j = 0; j < 2; j=j+1) {
            for (k = 0; k < 2; k=k+1) {
                putint(i * 100 + j * 10 + k); // 输出 000, 001, 010, ...
            }
        }
    }

    return 0;
}

// 测试样例：extreme_recursion.sy

int factorial(int n) {
    if (n <= 1) return 1;
    return n * factorial(n - 1);
}

int main() {
    int result = factorial(10); // 计算 10 的阶乘
    putint(result); // 输出 3628800

    return 0;
}

