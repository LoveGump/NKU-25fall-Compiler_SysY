// 测试样例：increment_decrement.sy

int main() {
    int x = 0;
    int y = 10;

    // 测试加法和减法替代自增自减
    x = x + 1;
    x = x + 1;

    y = y - 1;
    y = y - 1;

    // 输出结果
    putint(x); // 应输出 2
    putint(y); // 应输出 8

    return 0;
}